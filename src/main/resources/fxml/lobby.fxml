<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="728.0" prefWidth="1031.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.gui.LobbyController">
   <children>
      <HBox prefHeight="728.0" prefWidth="1031.0">
         <children>
            <VBox prefHeight="728.0" prefWidth="232.0">
